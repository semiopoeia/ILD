TITLE:
DSEM with Continuous Time

DATA:
FILE=
<<insert own, .txt or .dat>>

VARIABLE:
NAMES=
IDNo
Visit
Age
BMI
PA
CRDRot
CVLtca
CVLfrl
DigFor
DigBac
DSSTot
MMSTot
BOSCor
CLK325
CLK1110
FLUCat
FLULet
educ_years
MCI_dement
followupyrs
sex
TRATSr
TRBTSr
;

USEVAR=
IDNo 
followupyrs
PA
CRDRot
CVLtca
CVLfrl
DigFor
DigBac
DSSTot
BOSCor
CLK325
CLK1110
FLUCat
FLULet
TRATSr
TRBTSr
;

CLUSTER=IDNo;
TINTERVAL=followupyrs(1);
MISSING = ALL(-9999);
lagged= PA(1);

ANALYSIS: 
TYPE=TWOLEVEL RANDOM;
ESTIMATOR=BAYES;
BITER=(2000);
PROCESSORS=8;
                   
MODEL: 
    %WITHIN%
G BY CRDRot-TRBTSr@1(&1);
!PA BY PA_mse_mean@1(&1);
PAB| PA;
!CVLtca WITH CVLfrl;
!TRATSr WITH TRBTSr;
!DigFor WITH DigBac;
!CLK1110 WITH CLK325;
!FLUCat WITH FLULet;



bGG | G ON G&1;
bPP | PA ON PA&1;
bGP | G ON PA&1 ;
bPG | PA ON G&1 ;
!PA_mse_mean-TRBTSr PWITH PA_mse_mean&1-TRBTSr&1;

    %BETWEEN%
GB BY CRDRot-TRBTSr*;
!PAB BY PA_mse_mean@1;

GB@1; PAB (vpab);
bGG(arg); bPP(arp); bGP(clgp);bPG(clpg);
GB PAB bGG bPP bGP bPG WITH GB PAB bGG bPP bGP bPG;
  
Model Prior:
  arg~N(0,1);
  arp~N(0,1);
  clgp~N(0,1);
  clpg~N(0,1);
  vpab~IG(1,1);

  OUTPUT:	TECH1 TECH8;

